@Library('Jenkins_Shared_LIB') _

pipeline {
    agent any

    stages {
        stage('Git Checkout') {
            steps {
                gitCheckout(
                    branch:"sketch"
                    url: 'https://github.com/diamondhanddevOps/java-app.git'
                )
                }
            }
            stage('Unit Test maven'){
          
            steps{
               script{
                    
                    mvnTest()
            }   }    
        
        }       
    }  
}
